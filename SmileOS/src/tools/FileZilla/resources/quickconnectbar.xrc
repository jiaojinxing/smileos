<?xml version="1.0" encoding="ISO-8859-1"?>
<resource version="2.3.0.1" xmlns="http://www.wxwindows.org/wxxrc">
  <object class="wxPanel" name="ID_QUICKCONNECTBAR">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem" platform="win|unix">
        <flag>wxGROW</flag>
        <border>0</border>
        <object class="wxStaticLine" name="wxID_STATIC">
          <size>-1,-1</size>
          <style>wxLI_HORIZONTAL</style>
        </object>
      </object>
      <object class="sizeritem">
        <border>0</border>
        <object class="wxFlexGridSizer">
          <rows>1</rows>
          <vgap>0</vgap>
          <hgap>0</hgap>
          <object class="sizeritem" platform="win|unix">
            <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxTOP|wxBOTTOM</flag>
            <border>3</border>
            <object class="wxStaticLine" name="wxID_STATIC">
              <size>-1,-1</size>
              <style>wxLI_VERTICAL</style>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxTOP|wxBOTTOM</flag>
            <border>5</border>
            <object class="wxStaticText" name="wxID_STATIC">
              <size>-1,-1</size>
              <label>&amp;Host:</label>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxTextCtrl" name="ID_QUICKCONNECT_HOST">
              <size>110,-1</size>
              <value></value>
              <style>wxTE_PROCESS_ENTER</style>
              <tooltip>Enter the address of the server.
To specify the server protocol, prepend the host with the protocol identifier. If no protocol is specified, the default protocol (ftp://) will be used.
You can also enter complete URLs in the form protocol://user:pass@host:port here, the values in the other fields will be overwritten then.</tooltip>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxTOP|wxBOTTOM</flag>
            <border>5</border>
            <object class="wxStaticText" name="wxID_STATIC">
              <size>-1,-1</size>
              <label>&amp;Username:</label>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxTextCtrl" name="ID_QUICKCONNECT_USER">
              <size>-1,-1</size>
              <style>wxTE_PROCESS_ENTER</style>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxTOP|wxBOTTOM</flag>
            <border>5</border>
            <object class="wxStaticText" name="wxID_STATIC">
              <size>-1,-1</size>
              <label>Pass&amp;word:</label>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxTextCtrl" name="ID_QUICKCONNECT_PASS">
              <style>wxTE_PASSWORD|wxTE_PROCESS_ENTER</style>
              <size>-1,-1</size>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxTOP|wxBOTTOM</flag>
            <border>5</border>
            <object class="wxStaticText" name="wxID_STATIC">
              <size>-1,-1</size>
              <label>&amp;Port:</label>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxTextCtrl" name="ID_QUICKCONNECT_PORT">
              <size>45,-1</size>
              <style>wxTE_PROCESS_ENTER</style>
              <tooltip>Enter the port on which the server listens.
The default for FTP is 21, the default for SFTP is 22.</tooltip>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>0</border>
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                <border>0</border>
                <object class="wxButton" name="ID_QUICKCONNECT_OK">
                  <default>1</default>
                  <size>-1,-1</size>
                  <label>&amp;Quickconnect</label>
                </object>
              </object>
              <object class="sizeritem">
                <flag>wxALL|wxGROW|wxALIGN_CENTRE_VERTICAL</flag>
                <border>0</border>
                <object class="wxBitmapButton" name="ID_QUICKCONNECT_DROPDOWN">
                  <size>-1,-1</size>
                  <style>wxBU_AUTODRAW|wxBU_EXACTFIT</style>
                  <bitmap>dropdown.png</bitmap>
                </object>
              </object>
            </object>
          </object>
        </object>
      </object>
    </object>
    <pos>0, 0</pos>
  </object>
</resource>